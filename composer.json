		"phpunit/phpunit":"~4.8",
		"phpunit/phpunit-selenium" : "*",
		"sebastian/phpcpd": "*",
		"wp-cli/wp-cli":"*"
	},
	"extra": {
		"wordpress-install-dir": ".build/wordpress"
	}
}
